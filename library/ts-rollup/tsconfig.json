{
  "compilerOptions": {
    "outDir": "./build/lib",
    "target": "ES2018",
    "module": "ES2015",
    "lib": [
      "ESNext",
      "dom"
    ],
    "moduleResolution": "node",
    "typeRoots": [
      "node_modules/@types",
      "node_modules/@typed-mxgraph"
    ],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "strictBindCallApply": true,
    "alwaysStrict": true,
    "strictFunctionTypes": true,
    "noImplicitThis": true,
    "allowSyntheticDefaultImports": true
  },
}
